                        </td>
                        <td>
                            <%= objectbuild.surveyorb %>
                        </td>
                        <td>
                            <%= objectbuild.plusb %>
                            <% if objectbuild.objectplusrates == [] %>
                                <%= ' ' %>
                            <% else %>
                                <% @personbplus=objectbuild.objectplusrates.first.personbplus %>
                                <%= @personbplus %>
                            <% end %>
                        </td>
                        <td>
                            <% @totalmesa = testvalue(objectbuild.totalprice.to_f/objectbuild.buildarea.to_f,@personaplus,@personbplus)*10000 %>
                            <%= number_to_currency(@totalmesa, unit: "", precision: 0 ) %>
                        </td>
                        <td>
                            <% if objectbuild.objectplusrates == [] %>
                                 <%= link_to "勘查員A", new_yfcase_objectbuild_objectplusrate_path(@yfcase,objectbuild),class: "btn btn-danger btn-sm" %>
                            <% else %>
                                <%= link_to "勘查員A", new_yfcase_objectbuild_objectplusrate_path(@yfcase,objectbuild), :disabled => true, class: "btn btn-danger btn-sm" %>
                            <% end %>
                        </td>
                    </tr>
                    <% end %>
