                        <th>加成A</th>
                        <th>勘查員B</th>
                        <th>加成B</th>
                        <th>計算</th>
                        <th>加成A</th>
                    </tr>
                    <% @yfcase.objectbuilds.each do |objectbuild| %>
                    <tr>
                        <td>
                            <% if objectbuild.objectbuildurl == "" %>
                            <%= link_to "" %>
                            <% else %>
                            <%= link_to "連結", objectbuild.objectbuildurl, target: "_blank" %>
                            <% end %>
                        </td>
                        <td>
                            <%= objectbuild.address %>
                        </td>
                        <td>
                            <%= objectbuild.totalprice %>
                        </td>
                        <td>
                            <%= objectbuild.buildarea %>
                        </td>
                        <td>
                            <%= holdingpoint(objectbuild.totalprice,objectbuild.buildarea).round(2) %>
                        </td>
                        <td>
                            <%= objectbuild.house %>
                        </td>
                        <td>
                            <%= objectbuild.unit %>
                        </td>
                        <td>
                            <%= objectbuild.floorheight %>
                        </td>
                        <td>
                            <%= objectbuild.surveyora %>
                            <% if objectbuild.objectplusrates == [] %>
                                <%= ' ' %>
                            <% else %>
                                <% @persona= objectbuild.objectplusrates.first.persona %>
                                <%= @persona %>
                            <% end %>
                        </td>
                        <td>
                            <%= objectbuild.plusa %>
                            <% if objectbuild.objectplusrates == [] %>
                                <%= ' ' %>
                            <% else %>
                                <% @personaplus=objectbuild.objectplusrates.first.personaplus %>
                                <%= @personaplus %>
                            <% end %>
