<div class="nested-fields">
    <div class="field" style="border:1px solid #000;">
        <table>
            <tr>
                <th style="width:20%"><%= f.label :landnumber,"地號", :style => "width: 100%" %></th>
                <th style="width:12%"><%= f.label :landarea,"地坪(平方公尺)", :style => "width: 100%" %></th>
                <th style="width:7%"><%= f.label :landholdingpointperson,"持分(個人)", :style => "width: 100%" %></th>
                <th style="width:7%"><%= f.label :landholdingpointall,"持分(所有)", :style => "width: 100%" %></th>
                <th style="width:46%"><%= f.label :landurl,"地號謄本", :style => "width: 100%" %></th>
                <th style="width:8%">項目</th>
            </tr>
            <tr>
            	<td><%= f.text_field :landnumber, :style => "width: 100%" %></td>
            	<td><%= f.number_field :landarea, :step => 0.01, :style => "width: 100%" %></td>
            	<td><%= f.number_field :landholdingpointperson, :style => "width: 100%" %></td>
            	<td><%= f.number_field :landholdingpointall, :style => "width: 100%" %></td>
            	<td><%= f.url_field :landurl, :style => "width: 100%" %></td>
            	<td><%= link_to_remove_association "刪除地號", f, class: "btn btn-danger btn-sm" %></td>
            </tr>
        </table>
    	 
    </div>
</div>


